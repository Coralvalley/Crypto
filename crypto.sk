function CryptoPlayerData(p: player):
    set {player} to {_p}
    set {bitcoin.%{player}%} to yaml value "bitcoin" in "crypto-%uuid of {player}%"
    set {ethereum.%{player}%} to yaml value "ethereum" in "crypto-%uuid of {player}%"
