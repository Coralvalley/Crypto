    if file path "plugins/crypto/transactions.json" doesn't exist:
        create file path "plugins/crypto/transactions.json"

    
function transaction(p: player, coin: text, amount: number):
    clear {transaction::*}
    set {player} to {_p}
    set {coin.%{player}%} to {_coin}
    set {amount.%{player}%} to {_amount}

    # set {_json} to "{""name"": ""%{player}'s name%"", ""information"": { ""coin"":""%{coin.%{player}%}%"", ""amount"":""%{amount.%{player}%}%""}}"

    send a "post" request to "https://crypto-playerdata.silvinvanhaestr.repl.co/transactions.php" with the body "{""name"":%{player}'s name%,""information""}" and the headers "Authorization: %{api_key.%{player}%}%"
    # send a "post" request to "https://crypto-playerdata.silvinvanhaestr.repl.co/transactions.php" with the body "{""name"": ""%{player}'s name%"", ""information"": { ""coin"":""%{coin.%{player}%}%"", ""amount"":""%{amount.%{player}%}%""}}" and the headers "Authorization: %{api_key.%{player}%}%"

